<!DOCTYPE html>
<html lang="">

<head>
    <% include ../head %>
        <style>

        </style>
</head>

<body style="background-color:whitesmoke">

    <div class="container" style="background-color:#eee">

        <!-- head -->
        <% include ../pageheader %>


            <% include navbar %>


                <div class="row" style="height:10px;">

                </div>

                <!-- body -->
                <div class="row">


                    <form action="#" method="POST" class="form-horizontal" role="form" style="width:600px;">
                        <div class="input-group">
                            <input type="text" class="form-control" id="exampleInputAmount" placeholder="Search">
                            <span class="input-group-btn">
                    <button type="submit" class="btn btn-default"><span class="glyphicon glyphicon-search"></span></button>
                        </div>
                    </form>

                    <table class="table table-hover">
                        <thead>
                            <tr>
                                <th>负责人</th>
                                <th>名称</th>
                                <th>经费</th>
                                <th>导师</th>
                                <th>操作</th>
                                <th>审评结果</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% for(var i in projects){%>
                                <tr>
                                    <td>
                                        <%= projects[i]['_header'] %>
                                    </td>
                                    <td>
                                        <%= projects[i].name %>
                                    </td>
                                    <td>
                                        <%= projects[i].money %>
                                    </td>
                                    <td>
                                        <%= projects[i].teacher %>
                                    </td>
                                    <td><a data-toggle="modal" data-target="#pdf" href="/project/<%= projects[i]._id %>">详情</td>
                        <td><%= projects[i].accept.length %>/<%= projects[i].refuse.length %>/<%= projects[i].refuse.length+projects[i].accept.length %></td>
                    </tr>
                    <% } %>
                </tbody>
            </table>

        </div>


    </div>



    <!-- foot -->
    <div class="row">
    </div>


    </div>
        <div class="modal fade" id="pdf" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content"></div>
        </div>
    </div>
    <% include ../sameScript %>
    <script src="/js/jquery.media.js"></script>





    
</body>

</html>